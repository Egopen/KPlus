    "/Franchisto/Statistics/AddStatistics": {
      "put": {
        "tags": [
          "Statistics"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddStatsDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddStatsDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddStatsDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Franchisto/Statistics/AddStatisticsToDocs": {
      "put": {
        "tags": [
          "Statistics"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddStatsToDocsDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddStatsToDocsDTO"
              }
